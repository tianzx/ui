{
  "presets": [
    [
      "es2015",
      {
        "modules": false
      }
    ],
    "react",
    "stage-0"
  ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "lodash",
    [
      "import",
      {
        "libraryName": "antd",
        "style": true,
        "libraryDirectory": "lib"
      }
    ]
  ]
}
