{"name": "ui",
  "version": "2.0.0",
  "description": "frontend template to separate with backend",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": "7.0.0",
    "npm": "4.0"
  },
  "author": "tianzx",
  "keywords": [
    "react",
    "redux",
    "router",
    "antdesign",
    "webpack",
    "eslint",
    "babel",
    "es6",
    "hot-updte",
    "webpack-optimize"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tianzx/ui.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "test": "mocha tests/testSetup.js \"src/**/*.spec.js\"",
    "test-mocha": "mocha tests/testSetup.js tests/*-test.js",
    "lint": "esw webpack.devleop.config src ",
    "lint:watch": "npm run lint -- --watch",
    "build": "npm run clean && webpack --config ./webpack.production.config.js --colors --profile --display-modules",
    "build-dashboard": "npm run clean && webpack-dashboard --config ./webpack.production.config.js ",
    "start": "node server",
    "develop": "npm run start",
    "production": "export NODE_ENV=production&&node server",
    "production-cn": "node server production-cn",
    "production-ge": "node server production-ge"
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.13.4",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "qs": "^6.3.0",
    "request": "^2.76.0"
  },
  "devDependencies": {
    "antd": "^2.9.0",
    "babel-core": "^6.5.2",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^7.0.0-beta.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "babel-runtime": "^6.23.0",
    "webpack-uglify-parallel":"^0.1.3",
    "babel-plugin-lodash":"^3.2.11",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "history": "^4.6.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.28.0",
    "js-cookie": "^2.1.2",
    "less": "^2.7.2",
    "less-loader": "^ 4.0.3",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-google-maps": "^6.0.1",
